# Start with all the queries in the codeql/cpp-queries pack.
- queries: .
  from: codeql/cpp-queries
# Restrict to only the queries with the following ID patterns.
- include:
    id:
      - cpp/jpl-c/preprocessor-use
      - cpp/jpl-c/preprocessor-use-ifdef
      - cpp/jpl-c/preprocessor-use-partial
      - cpp/jpl-c/preprocessor-use-undisciplined